{
  "name": "ai-pulp-ui",
  "version": "1.0.0",
  "description": "React UI for Pulp Project",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --run --coverage",
    "test:deb": "vitest --run src/test/Deb*.test.tsx",
    "test:deb:coverage": "vitest --run --coverage src/test/Deb*.test.tsx",
    "test:rpm": "vitest --run src/test/Rpm*.test.tsx",
    "test:rpm:coverage": "vitest --run --coverage src/test/Rpm*.test.tsx",
    "test:e2e": "playwright test",
    "test:e2e:deb": "playwright test e2e/deb.spec.ts",
    "test:e2e:rpm": "playwright test e2e/rpm.spec.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "playwright:install": "playwright install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.0.4",
    "jsdom": "^23.0.1",
    "playwright": "^1.57.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  }
}
